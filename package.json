    "next-seo": "^5.15.0",
    "nprogress": "^0.2.0",
    "prism-react-renderer": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-markdown": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
